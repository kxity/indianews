        try {
            if (window.sdLoggerSwitch && typeof SDLogger === "object" && typeof SDLogger.log === 'function') {
                SDLogger.init(window.SDLoggerConfig || {});
                var params = { statusCode: "400" };
                var pathname = window.location.pathname;
